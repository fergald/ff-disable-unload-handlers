{

  "manifest_version": 2,
  "name": "UnloadDisabler",
  "version": "1.0",
  "permissions": [
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "contextMenus",
    "tabs",
    "scripting",
    "webRequest",
    "webRequestBlocking"
  ],

  "description": "Disables unload and beforeunload handlers. Useful when debugging BFCache.",

  "background": {
    "scripts": ["background.js"]
  }

}
